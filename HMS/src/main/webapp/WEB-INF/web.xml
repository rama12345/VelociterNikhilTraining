<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">

  <display-name>HMS</display-name>
  <absolute-ordering />
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
     <welcome-file>AddPhysician.jsp</welcome-file>
  </welcome-file-list>
 
   <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.velociter.hms.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>AddPatientServlet</servlet-name>
    <servlet-class>com.velociter.hms.controller.AddPatientServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddPatientServlet</servlet-name>
    <url-pattern>/AddPatientServlet</url-pattern>
  </servlet-mapping>
  
     <servlet>
    <servlet-name>AddNurseServlet</servlet-name>
    <servlet-class>com.velociter.hms.controller.AddNurseServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddNurseServlet</servlet-name>
    <url-pattern>/AddNurseServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>AddDoctorServlet</servlet-name>
    <servlet-class>com.velociter.hms.controller.AddDoctorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddDoctorServlet</servlet-name>
    <url-pattern>/AddDoctorServlet</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>DepartmentServlet</servlet-name>
    <servlet-class>com.velociter.hms.controller.DepartmentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DepartmentServlet</servlet-name>
    <url-pattern>/DepartmentServlet</url-pattern>
  </servlet-mapping>
  
 <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.velociter.hms.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  </web-app>